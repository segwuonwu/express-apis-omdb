<% // Render your search results here %>
    <% // This is how to do comments in EJS, by the way... %>

        <h1>Results</h1>
        <% movies.forEach(function(movie, idm) { %>
            <div class="movie">
                <h3>
                    <a href="/movies/<%= idm %>">
                        <%= movie.Title %>
                    </a>
                </h3>

            </div>
            <% }) %>